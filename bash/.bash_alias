#User specific aliases

alias chrome="google-chrome --incognito &"
alias sublime="sublime_text &"
alias home="cd ~"
alias hm=home
alias hoem=home
alias c="clear"
alias lsa="ls -a"
alias ll="ls -lhA"
alias lla="ll -a"
alias mkdir="mkdir -pv"
alias vi=vim
alias ff="find .-name $1"
